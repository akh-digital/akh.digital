---
import { about } from "../../lib/config.js";
import Icon from "../Icon.astro";
import ResumesLink from "./ResumesLink.astro";
import Links from "./components/Links.astro";

const cv = about().cv;
const list = cv.list;
---

<Links>
	<Fragment slot="header">
		{cv.title}
	</Fragment>
	<Fragment slot="second-title">
		{cv.second_title}
	</Fragment>
	<Fragment slot="links-list">
		{list.map((cv) => <ResumesLink url={cv.url} title={cv.title} />)}
	</Fragment>
</Links>
